<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.yosakura.entity.Message" table="t_message" catalog="task0804">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="sender_uid" column="sender_uid"/>
		<property name="recipient_uid" column="recipient_uid"/>
		<property name="create_date" generated="insert" not-null="true" > 
            <column name="create_date" sql-type="timestamp" default="CURRENT_TIMESTAMP" /> 
        </property> 
		<property name="title" column="title"/>
		<property name="content" column="content"/>
		<property name="type" column="type"/>
	</class>
</hibernate-mapping>