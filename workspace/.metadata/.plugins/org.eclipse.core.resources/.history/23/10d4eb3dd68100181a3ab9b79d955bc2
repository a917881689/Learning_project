        for (int i = 0;i < infoValue.getValue();i++) {
            String[] strInfo = str[i].split(":");
            if (!"-1".equals(strInfo[1]) && !"null".equals(strInfo[1]) && !"-1.0".equals(strInfo[1]) ) {
                objArray[i] = strInfo[1];
            }else {
