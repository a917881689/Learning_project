package com.yosakura.entity;

public class GType {
	private long tid;
	private String tname;
	public GType(long tid, String tname) {
		super();
		this.tid = tid;
		this.tname = tname;
	}
	public GType() {
		super();
	}
	@Override
	public String toString() {
		return "Type [tid=" + tid + ", tname=" + tname + "]";
	}
	public long getTid() {
		return tid;
	}
	public void setTid(long tid) {
		this.tid = tid;
	}
	public String getTname() {
		return tname;
	}
	public void setTname(String tname) {
		this.tname = tname;
	}
	
}
