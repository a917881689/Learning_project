<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>转账页面</title>
<script type="text/javascript" src="./js/jquery-3.1.0.min.js"></script>
</head>
<body>
	<form action="zhuanzhang" method="post" onsubmit="return check();">
		付款人: <input type="text" name="from"/>
		收款人: <input type="text" name="to"/>
		金额: <input type="text" name="money"/>
		<input type="submit" value="开始转账"/>
	</form>
<script type="text/javascript">
	function check() {
		var $form = $('#from').val();
		if ('' == $form) {
			alert("收款人不能为空")
			return;
		}
		var $to = $('#to').val
		if ('' == $to) {
			alert("收款人不能为空")
			return;
		}
		var $money = $('#money').val
		if ('' == $money) {
			alert("金额不能为空")
			return;
		}
		if (isNaN($money)) {
			alert("金额不能为空")
			return;
		}
		return true;
	}		
</script>
</body>
</html>