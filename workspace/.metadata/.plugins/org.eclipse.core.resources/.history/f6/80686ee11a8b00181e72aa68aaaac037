package com.yosakura.test;

public class IntanceTest {
	public static void main(String[] args) {
		// 现在只有Class字节码文件,如何通过字节码创建对象
		// 1>获取字节码对象
		try {
			Class<?> clazz = Class.forName("com.yosakura.entity.User");
			clazz.newInstance();
		} catch (Exception e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
}
