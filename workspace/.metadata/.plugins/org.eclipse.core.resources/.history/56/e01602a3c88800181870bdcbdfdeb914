	<%
	request.setAttribute("uname","admin");
	// 2.在session域中存放【对象】
	Emp e1 = new Emp(1,"张三",6666);
	session.setAttribute("emp",e1);
	// 3.在request中存放【对象集合】
	ArrayList<Emp> list = new ArrayList<Emp>();
	list.add(e1);
	list.add(new Emp(2,"李四",8888));
	%>
