package com.yosakura.entity;

public class Goods {
	private long gId;
	private String gName;
	private String type;
	private double price;
	public Goods() {
		super();
	}
	public Goods(long gId, String gName, String type, double price) {
		super();
		this.gId = gId;
		this.gName = gName;
		this.type = type;
		this.price = price;
	}
	public Goods(String gName, String type, double price) {
		super();
		this.gId = -1;
		this.gName = gName;
		this.type = type;
		this.price = price;
	}
	@Override
	public String toString() {
		return "Goods [gId=" + gId + ", gName=" + gName + ", type=" + type + ", price=" + price + "]";
	}
	public long getgId() {
		return gId;
	}
	public void setgId(long gId) {
		this.gId = gId;
	}
	public String getgName() {
		return gName;
	}
	public void setgName(String gName) {
		this.gName = gName;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public double getPrice() {
		return price;
	}
	public void setPrice(double price) {
		this.price = price;
	}
	
}
