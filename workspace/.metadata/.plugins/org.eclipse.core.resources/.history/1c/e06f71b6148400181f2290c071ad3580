package com.yosakura.web.Util;

import java.sql.ResultSet;
import java.sql.SQLException;

/**
 * @Author: yoSakura
 * @Date: 2018/6/13 14:03
 */
public interface ResultSetHandler <T>{
    /**
     * 处理SQL查询结果集
     * @param result 结果集
     * @return 返回设置的对象
     * @throws SQLException
     */
    T handlerRow(ResultSet result) throws SQLException;
}
