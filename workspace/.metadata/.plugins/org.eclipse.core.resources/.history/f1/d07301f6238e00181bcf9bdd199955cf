package com.yosakura.entity;

import java.util.List;

public class ProCategoryModel {
	private ProductCategory Product;
	private List<ProCategoryModel> son;
	
	public ProCategoryModel() {
		super();
	}
	public ProCategoryModel(ProductCategory product, List<ProCategoryModel> son) {
		super();
		Product = product;
		this.son = son;
	}
	
	@Override
	public String toString() {
		return "ProCategoryModel [Product=" + Product + ", son=" + son + "]";
	}
	public ProductCategory getProduct() {
		return Product;
	}
	public void setProduct(ProductCategory product) {
		Product = product;
	}
	public List<ProCategoryModel> getSon() {
		return son;
	}
	public void setSon(List<ProCategoryModel> son) {
		this.son = son;
	}
	
}
