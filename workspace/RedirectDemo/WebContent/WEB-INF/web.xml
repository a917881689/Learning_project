<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>DispatcherDemo</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>GuanYin</servlet-name>
  	<servlet-class>com.yosakura.web.Servlet.GuanYinServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GuanYin</servlet-name>
  	<url-pattern>/helpGuanYin</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>NiuMo</servlet-name>
  	<servlet-class>com.yosakura.web.Servlet.NiuMoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>NiuMo</servlet-name>
  	<url-pattern>/niuMo</url-pattern>
  </servlet-mapping>
</web-app>